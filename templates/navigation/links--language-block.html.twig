{#
/**
 * @file
 * Theme override for a set of links used in the language switcher block.
 *
 * @see ./core/modules/stable/navigation/links.html.twig
 */
#}
{{ attach_library('oe_theme/dialogs') }}
{% if links -%}
  {% include '@ecl/language-list/language-list-overlay' with {
    items: languages,
    icon_path: icon_path,
    title: 'Select your language'|t,
    close_label: 'Close'|t,
    extra_classes: 'ecl-site-header__language-list',
  } %}
{%- endif %}
